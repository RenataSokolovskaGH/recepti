{
  "name": "my-ideal-recipe",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src",
  "scripts": {
    "start": "node dist/src",
    "build": "npx tsc",
    "build:clear": "rimraf dist",
    "start:dev": "ts-node src",
    "migrate:rollback": "npx knex migrate:rollback --knexfile=./knexfile.ts",
    "migrate:latest": "npx knex migrate:latest --knexfile=./knexfile.ts",
    "migrate:make": "cross-env NODE_ENV=make npx knex migrate:make -x ts --knexfile=./knexfile.ts",
    "seed:run": "npx knex seed:run --knexfile=./knexfile.ts"
  },
  "author": "Renata Sokolovska",
  "license": "See License",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "objection": "^3.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/swagger-ui-express": "^4.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}