{
  "name": "recipes",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src",
  "scripts": {
    "start": "NODE_PATH=dist/ node dist/src",
    "build": "npx tsc",   
    "build:clear": "rimraf dist",
    "start:dev": "NODE_PATH=./ ts-node src",
    "migrate:rollback": "NODE_PATH=dist/ npx knex migrate:rollback --knexfile=./knexfile.ts",
    "migrate:latest": "NODE_PATH=dist/ npx knex migrate:latest --knexfile=./knexfile.ts",
    "migrate:make": "cross-env NODE_ENV=make npx knex migrate:make -x ts --knexfile=./knexfile.ts",
    "seed:run": "cross-env NODE_ENV=make npx knex seed:run --knexfile=./knexfile.ts"
  },
  "author": "Renata",
  "license": "See License",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mysql2": "^2.2.5",
    "objection": "^3.0.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "sharp": "^0.30.6"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@types/sharp": "^0.30.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint": "^8.26.0",
    "express-basic-auth": "^1.2.1",
    "express-useragent": "^1.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}